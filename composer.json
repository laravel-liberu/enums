        "enums"
    ],
    "homepage": "https://github.com/laravel-liberu/enums",
    "type": "library",
    "license": "MIT",
    "authors": [{
            "name": "Reza Fouladi",
            "email": "raftx24@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        },
        {
            "name": "Adrian Ocneanu",
